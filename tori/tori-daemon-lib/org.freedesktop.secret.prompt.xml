<node>
  <interface name="org.freedesktop.Secret.Prompt">
    <method name="Prompt">
        <arg name="window_id" type="s" direction="in"/>
    </method>
    <method name="Dismiss" />
    <signal name="Completed">
        <arg name="dismissed" type="b" direction="out"/>
        <arg name="result" type="v" direction="out"/>
    </signal>
  </interface>
</node>
