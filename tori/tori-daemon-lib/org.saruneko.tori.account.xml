<node>
  <interface name="com.saruneko.tori.Account">
    <method name="setPin">
        <arg name="pin" type="s" direction="in" />
    </method>
    <method name="authenticate"/>
    <method name="isAuthenticated"/>
    <signal name="oauthPinUrl">
        <arg name="pinUrl" type="s" direction="out"/>
        <arg name="username" type="s" direction="out"/>
    </signal>
    <signal name="authenticated">
        <arg name="authenticated" type="b" direction="out"/>
        <arg name="username" type="s" direction="out"/>
    </signal>
    <signal name="authenticationError">
        <arg name="error" type="u" direction="out"/>
    </signal>
 </interface>
</node>
